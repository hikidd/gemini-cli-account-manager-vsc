{
  "name": "gemini-cli-account-manager",
  "displayName": "Gemini CLI Account Manager",
  "description": "Manage multiple Google accounts for Gemini CLI.",
  "version": "1.0.6",
  "publisher": "hi-kidd",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "gemini",
    "cli",
    "account",
    "manager"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hikidd/gemini-cli-account-manager-vsc.git"
  },
  "activationEvents": [
    "onView:gemini-manager.sidebarView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gemini-manager-sidebar",
          "title": "Gemini Manager",
          "icon": "resources/sidebar-icon.svg"
        }
      ]
    },
    "views": {
      "gemini-manager-sidebar": [
        {
          "type": "webview",
          "id": "gemini-manager.sidebarView",
          "name": "Account Manager"
        }
      ]
    },
    "commands": [
      {
        "command": "gemini-manager.openPanel",
        "title": "Open Gemini Account Manager",
        "category": "Gemini Manager",
        "icon": "$(account)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.80.0",
    "@vscode/vsce": "^2.19.0",
    "dotenv-webpack": "^8.1.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.0.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.4"
  }
}
